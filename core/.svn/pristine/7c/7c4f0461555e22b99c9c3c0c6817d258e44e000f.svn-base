.table > tbody > tr {
    background-color: #f7f9fa;
    margin: 5px;
    border-top: 3px solid #ffffff;
}

tr.odd, tr.odd a{
/*    font-size:12px;*/
    font-weight: 700;
    background-color:#f7f9fa;
}
.table > tbody > tr.odd{
    overflow:hidden;
    border-top: none;
}

.table > tbody > tr.even{
    overflow:hidden;
    background-color: #eeeeee;
    border-top: none;
}

tr.odd:hover, div.odd:hover a
{
    background-color:#eeeeee;
    cursor: pointer;
    color:#474747;
}

tr.even, tr.even a{
/*    font-size:12px;*/
    font-weight: 700;
    background-color:#eeeeee;
}

tr.even:hover, tr.even:hover a
{
    background-color:#f7f9fa;
    cursor: pointer;
    color:#474747;
}

