#affidavitOverlay .close {
  background-image:url('/core/webassets/images/close.png');
  position:absolute;
  right:-15px;
  top:-15px;
  cursor:pointer;
  height:35px;
  width:35px;
}

#affidavitOverlayContent {
  padding: 0 10px 10px 10px;
}

